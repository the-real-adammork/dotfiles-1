#My Scripts

export PATH=$PATH:.
#export PATH=$PATH:~/.dotfiles/scripts

# Add brew's executables to front of path to override system ones
#export PATH=/usr/local/sbin:$PATH
#export PATH=/usr/local/bin:$PATH
#export PATH=$(brew --prefix)/bin:$(brew --prefix)/sbin:$PATH

# Add Google Cloud /bin to PATH.
export PATH="$PATH:$HOME/Development/google-cloud-sdk/bin"

# Add RVM to PATH for scripting. Make sure this is the last PATH variable change.
export PATH="$PATH:$HOME/.rvm/bin"

# Add Node & NPM DUPLICATE
#export PATH="/usr/local/bin/:$PATH"

# Add python to the path
export PATH="/usr/local/opt/python/libexec/bin:$PATH"

# Add lib_py to path for helper scripts
export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PATH:$HOME/.dotfiles/lib_py"

# Add lib_sh to path for helper scripts
export PATH="$PATH:$HOME/.dotfiles/lib_sh"

# Add pyenv
export PATH="$PYENV_ROOT/bin:$PATH"

# Add ptyhon2 from brew
export PATH="/usr/local/opt/python@2/libexec/bin:$PATH"

# Haskell stacks asked me to add this to my path
export PATH="$HOME/.local/bin:$PATH"

# Add class-dump from my dotfiles repo
export PATH="$HOME/.dotfiles/class-dump:$PATH"

# Add sourcekit-lsp
export PATH="$HOME/.dotfiles/sourcekit-lsp/.build/x86_64-apple-macosx/debug:$PATH"


